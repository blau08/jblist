{{model.name}}

<button {{action 'delete' model}}>Delete</button>
{{update-category category=model update='update'}}

{{add-listing category=model saveListing='saveListing'}}

<div id="name">
  <ul>
    {{#each model.listings as |listing|}}
      <li>
        {{display-listing listing=listing}}
      </li>
      {{/each}}
  </ul>
</div>
